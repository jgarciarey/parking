<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.parking.model">
	<class name="Vehiculo" table="vehiculo">
		<id name="id_vehiculo" type="integer" column="id_vehiculo">
			<generator class="increment"></generator>
		</id>

		<property name="tipo" type="string" column="tipo" />
		<property name="tarifa" type="double" column="tarifa" />
	</class>
</hibernate-mapping>
